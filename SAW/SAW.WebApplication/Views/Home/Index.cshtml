@{
    ViewBag.Title = "Home Page";
}

<input id="d121" class="form-control" type="text" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', isShowWeek: true})" />
<input id="d122" class="form-control" type="text" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', isShowWeek: true, onpicked: function () { $dp.$('d123').value = $dp.cal.getP('W','WW周'); }})" hidden />
<input id="d123" class="form-control" type="text" />
<input id="d124" class="form-control" type="text" onclick="WdatePicker({ dateFmt: 'yyyy年MM月', isShowWeek: true})" />
<input id="d125" class="form-control" type="text" onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', isShowWeek: true, onpicked: function () { $dp.$('d126').value = $dp.cal.getP('y','yyyy年') + Math.round(($dp.cal.date.M - 1) / 3 + 1).toString() + '季度'; }})" hidden />
<input id="d126" class="form-control" type="text" />
<input id="d127" class="form-control" type="text" onclick="WdatePicker({ dateFmt: 'yyyy年', isShowWeek: true})" />

@section scripts{
<script src="~/Scripts/My97DatePicker/WdatePicker.js"></script>
}